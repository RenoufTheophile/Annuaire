
@model Annuaire.Models.Root


@{ ViewData["Title"] = "Ramdom User"; }

<h1>Create</h1>

<h4>Personne</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            @foreach (var personnes in Model.results)
            {
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="@personnes.name.first" class="control-label"></label>
                <input asp-for="@personnes.name.first" class="form-control" />
                <span asp-validation-for="@personnes.name.first" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="@personnes.name.last" class="control-label"></label>
                <input asp-for="@personnes.name.last" class="form-control" />
                <span asp-validation-for="@personnes.name.last" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="@personnes.registered.date" class="control-label"></label>
                <input asp-for="@personnes.registered.date" class="form-control" />
                <span asp-validation-for="@personnes.registered.date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="@personnes.phone" class="control-label"></label>
                <input asp-for="@personnes.phone" class="form-control" />
                <span asp-validation-for="@personnes.phone" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="@personnes.personne.service" class="control-label"></label>
                <input asp-for="@personnes.personne.service" class="form-control" />
                <span asp-validation-for="@personnes.personne.service" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
            }
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
